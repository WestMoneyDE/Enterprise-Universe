{
  "name": "nexus-command-center",
  "version": "1.0.0",
  "private": true,
  "description": "Enterprise Universe - Nexus Command Center Platform",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "db:generate": "pnpm --filter @nexus/db generate",
    "db:migrate": "pnpm --filter @nexus/db migrate",
    "db:push": "pnpm --filter @nexus/db push",
    "db:studio": "pnpm --filter @nexus/db studio",
    "clean": "turbo clean && rm -rf node_modules",
    "typecheck": "turbo typecheck"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@vitest/coverage-v8": "^2.1.8",
    "prettier": "^3.2.4",
    "tsx": "^4.19.2",
    "turbo": "^2.3.0",
    "typescript": "^5.3.3",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "dependencies": {
    "stripe": "^20.2.0"
  }
}
