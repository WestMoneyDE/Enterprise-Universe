    "studio": "drizzle-kit studio",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "drizzle-orm": "^0.38.3",
    "pg": "^8.17.1",
    "postgres": "^3.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.30.1",
    "typescript": "^5.3.3"
  }
